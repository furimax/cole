<resources>


    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/material_blue_500</item>
        <item name="colorPrimaryDark">@color/material_blue_grey_800</item>
        <item name="colorAccent">@color/material_blue_grey_900</item>
        <item name="android:textColorSecondary">@color/textColorSecondary</item>
        <item name="colorControlHighlight">@color/material_blue_grey_950</item>
        <item name="android:windowBackground">@color/background</item>
    </style>

    <style name="AppTheme" parent="AppTheme.Base" />

    <style name="AppTheme.Detail">
        <item name="colorPrimaryDark">@android:color/black</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="actionBarStyle">@style/ActionBar.Detail</item>
    </style>

    <style name="ActionBar.Detail" parent="Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@null</item>
    </style>

    <style name="AppTheme.Login" parent="AppTheme.Base">
        <item name="android:windowBackground">@color/finanzasColor</item>
    </style>


    <style name="AppTheme.Login2" parent="AppTheme">
        <item name="android:windowBackground">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>



</resources>
